{
  "name": "today-i-learned",
  "version": "1.0.0",
  "description": "a boilerplate to quickly startup  a restapi with express",
  "main": "server.js",
  "scripts": {
    "start": "ts-node server/server.js",
    "start-server": "ts-node-dev --respawn --transpile-only  server/server.js",
    "test": "export NODE_ENV=test || SET NODE_ENV=test && jest tests/**/*.test.js --detectOpenHandles --forceExit",
    "test-watch": "ts-node-dev --exec \"npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaymondMik/GetRideApp.git"
  },
  "engines": {
    "node": "10.14.2"
  },
  "keywords": [
    "TypeScript",
    "Express",
    "Node.js",
    "REST",
    "API",
    "Mongodb",
    "Mongoose"
  ],
  "author": "Ramon Miklus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RaymondMik/express_restapi_boilerplate/issues"
  },
  "homepage": "https://github.com/RaymondMik/express_restapi_boilerplate#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.2",
    "jest": "^26.4.2",
    "pre-commit": "^1.2.2",
    "supertest": "^5.0.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  }
}
